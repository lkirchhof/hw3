<h1>New Post</h1>

<%= form_with :model => @post do |form| %>
  <p>
  <%= form.label "title"%>
  <%= form.text_field "title"%>
  </p>
  <p>  
  <%= form.label "description", "Caption"%>
  <%= form.text_field "description"%>  </p>
  </p>
  <p>
  <%= form.label "posted_on", "Date"%>
  <%= form.date_field "posted_on"%>
  </p>
  <p>
  <%= form.hidden_field "place_id"%>
  </p>
  <%= form.submit %>
<% end %>
